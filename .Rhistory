a = gsub(" |[A-Za-zÃ-ÈÃÃ ]|\n|.*\\)|\\*","",text)
a2 = gsub('\\/..', '', a)
pattern = regex("[0-9]{6,}")
b = str_extract_all(a2,pattern)
valores <- c()
for(i in 1:7){
for (x in b[[i]]){
if(nchar(x) == 6){
valores <- append(valores, substr(x,1,4))
} else if(nchar(x) == 8){
valores <-append(valores, substr(x,3,6))
} else{
for(i in seq(6,nchar(x), by=6)){
valores <- append(valores, substr(x,i-3,i))
}
}
}
}
